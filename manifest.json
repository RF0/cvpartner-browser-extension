{
  "manifest_version": 3,
  "name": "CV",
  "version": "1.0",
  "description": "CVPS - CV Partner Exctension",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "CV Partner Extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {                   
    "16": "images/CV16.png",
    "48": "images/CV48.png",
    "128": "images/CV128.png"  
  },    
  "browser_action": {
    "default_title": "CV Partner Extension",
    "default_icon": {                   
      "16": "images/CV16.png",
      "48": "images/CV48.png",
      "128": "images/CV128.png"     
    }
  }  
}
